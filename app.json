      "buildNumber": "1.0.1",
      "supportsTablet": false
    },
    "android": {
      "package": "com.mert.online_shop_demo",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/cart_icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/cart_icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      }
    }
  }
